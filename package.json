{
  "name": "fun-test",
  "version": "0.13.0",
  "description": "A simple function tester",
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tap -Rtap --coverage-report=text-summary ./test/index.js",
    "predependencies": "mkdir -p img",
    "dependencies": "madge . --image img/dependencies.svg -x 'test|coverage|docs'",
    "predependencies-test": "mkdir -p img",
    "dependencies-test": "madge . --image img/dependencies-test.svg -x 'coverage|docs'",
    "coverage": "tap --coverage-report=lcov ./test/index.js",
    "pages-index": "showdown makehtml -i README.md -o public/index.html",
    "pages": "./script/pages.sh",
    "lint": "eslint --ignore-path .gitignore --fix .",
    "document": "jsdoc --destination docs -c .jsdoc.json -r",
    "readme": "cat readme/* > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bagrounds/fun-test.git"
  },
  "keywords": [
    "test",
    "function",
    "simple"
  ],
  "author": "bagrounds <bagrounds@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/bagrounds/fun-test/issues"
  },
  "homepage": "https://gitlab.com/bagrounds/fun-test#README",
  "dependencies": {
    "data.task": "^3.1.1",
    "fun-arrow": "^0.2.0",
    "fun-assert": "latest",
    "fun-try": "^0.1.0",
    "guarded": "^0.4.1",
    "lodash.defaults": "^4.2.0",
    "ramda": "^0.23.0",
    "task-timeout": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "fun-test-runner": "latest",
    "fun-test-tests": "latest",
    "jsdoc": "^3.4.3",
    "showdown": "^1.5.1",
    "tap": "^9.0.3"
  }
}
